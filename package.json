{
  "name": "openservice",
  "version": "0.0.5",
  "description": "Flexible easy to use microservice framework on streams",
  "main": "index.js",
  "author": "david <daywiss@gmail.com>",
  "license": "MIT",
  "bin": {
    "openservice": "./bin/openservice.js"
  },
  "scripts": {
    "husky": "husky install",
    "build": "tsc",
    "watch": "tsc --watch",
    "e2e": "ts-node node_modules/tape/bin/tape src/**/*.e2e.ts",
    "test": "ts-node node_modules/tape/bin/tape src/*.test.ts src/**/*.test.ts",
    "start": "ts-node",
    "format": "prettier --write src",
    "lint": "eslint ./**/*.ts --fix"
  },
  "lint-staged": {
    "libs/**/*.js": [
      "eslint --ext .js",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": {
    "url": "https://github.com/daywiss/openservice/issues"
  },
  "homepage": {
    "url": "https://github.com/daywiss/openservice"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daywiss/openservice.git"
  },
  "keywords": [
    "services",
    "microservices",
    "streams",
    "servers",
    "framework",
    "events",
    "cqrs"
  ],
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.21.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "tape": "^5.5.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@types/highland": "^2.12.14",
    "@types/lodash": "^4.14.182",
    "@types/uuid": "^9.0.0",
    "bluebird": "^3.5.5",
    "fastest-validator": "^1.15.0",
    "highland": "^2.13.5",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "moment": "^2.24.0",
    "node-nats-streaming": "^0.3.2",
    "nuid": "^1.1.4",
    "openenv": "^0.0.1",
    "stack-utils": "^2.0.6"
  }
}
